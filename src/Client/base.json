{
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"erasableSyntaxOnly": true,
		"incremental": true,
		"module": "NodeNext",
		"noImplicitOverride": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "ESNext",
		"verbatimModuleSyntax": true
	}
}
